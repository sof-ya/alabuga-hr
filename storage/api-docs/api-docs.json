{
    "openapi": "3.0.0",
    "info": {
        "title": "API",
        "version": "1.0.0"
    },
    "paths": {
        "/api/auth/login": {
            "post": {
                "tags": [
                    "auth"
                ],
                "description": "Get a JWT via given credentials.",
                "operationId": "b648b90b9891b4b73ac9248388fbbc65",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "test@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "password"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "$ref": ""
                    },
                    "422": {
                        "$ref": ""
                    }
                }
            }
        },
        "/api/auth/me": {
            "post": {
                "tags": [
                    "auth"
                ],
                "description": "Get the authenticated User.",
                "operationId": "d47167655a419dad14940bf72c84ef82",
                "responses": {
                    "200": {
                        "description": "User",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/User"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/store": {
            "get": {
                "tags": [
                    "store"
                ],
                "summary": "Get store items",
                "operationId": "d3f1392f72e196a2fa920b7e7541085d",
                "parameters": [
                    {
                        "$ref": "#/components/parameters/page"
                    },
                    {
                        "$ref": "#/components/parameters/per_page"
                    },
                    {
                        "$ref": "#/components/parameters/in_storage"
                    },
                    {
                        "$ref": "#/components/parameters/only_available"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/StoreItem"
                                    }
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "JWT": []
                    }
                ]
            }
        },
        "/api/store/buy/{id}": {
            "patch": {
                "tags": [
                    "store"
                ],
                "summary": "Buy store item",
                "operationId": "04bf7316d035bce9a99ef21fed324e5a",
                "parameters": [
                    {
                        "$ref": "#/components/parameters/id"
                    }
                ],
                "responses": {
                    "400": {
                        "$ref": "#/components/responses/ErrorResponse"
                    },
                    "422": {
                        "$ref": "#/components/responses/ValidationErrorsResponse"
                    }
                },
                "security": [
                    {
                        "JWT": []
                    }
                ]
            }
        }
    },
    "components": {
        "schemas": {
            "ValidationErrors": {
                "required": [
                    "message",
                    "errors"
                ],
                "properties": {
                    "message": {
                        "type": "string"
                    },
                    "errors": {
                        "type": "object",
                        "example": {
                            "field": [
                                "string"
                            ]
                        }
                    }
                },
                "type": "object"
            },
            "LengthAwarePaginator": {
                "required": [
                    "current_page",
                    "data",
                    "first_page_url",
                    "from",
                    "last_page",
                    "last_page_url",
                    "links",
                    "next_page_url",
                    "path",
                    "per_page",
                    "prev_page_url",
                    "to",
                    "total"
                ],
                "properties": {
                    "current_page": {
                        "type": "integer",
                        "minimum": 1
                    },
                    "data": {
                        "type": "array",
                        "items": {
                            "type": "object"
                        }
                    },
                    "first_page_url": {
                        "type": "string"
                    },
                    "from": {
                        "type": "integer",
                        "minimum": 1
                    },
                    "last_page": {
                        "type": "integer",
                        "minimum": 1
                    },
                    "last_page_url": {
                        "type": "string"
                    },
                    "links": {
                        "type": "array",
                        "items": {
                            "required": [
                                "url",
                                "label",
                                "active"
                            ],
                            "properties": {
                                "url": {
                                    "type": "string",
                                    "nullable": true
                                },
                                "label": {
                                    "type": "string"
                                },
                                "active": {
                                    "type": "boolean"
                                }
                            },
                            "type": "object"
                        }
                    },
                    "next_page_url": {
                        "type": "string",
                        "nullable": true
                    },
                    "path": {
                        "type": "string",
                        "nullable": true
                    },
                    "per_page": {
                        "type": "integer",
                        "minimum": 1
                    },
                    "prev_page_url": {
                        "type": "string",
                        "nullable": true
                    },
                    "to": {
                        "type": "integer",
                        "minimum": 1
                    },
                    "total": {
                        "type": "integer",
                        "minimum": 1
                    }
                },
                "type": "object"
            },
            "StoreItem": {
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int64",
                        "minimum": 1
                    },
                    "name": {
                        "type": "string",
                        "maxLength": 255,
                        "example": "Футболка"
                    },
                    "descrirpion": {
                        "type": "string",
                        "example": "Классная футболка",
                        "nullable": true
                    },
                    "price": {
                        "type": "integer",
                        "format": "int4",
                        "minimum": 1
                    },
                    "image": {
                        "type": "string",
                        "example": "images/1.jpg",
                        "nullable": true
                    },
                    "items_count": {
                        "type": "integer",
                        "format": "int4",
                        "minimum": 1
                    },
                    "is_available": {
                        "type": "boolean"
                    },
                    "in_purchases": {
                        "type": "boolean"
                    },
                    "is_active": {
                        "type": "boolean"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "nullable": true
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time",
                        "nullable": true
                    },
                    "deleted_at": {
                        "type": "string",
                        "format": "date-time",
                        "nullable": true
                    }
                },
                "type": "object"
            },
            "User": {
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int64",
                        "minimum": 1
                    },
                    "name": {
                        "type": "string",
                        "format": "email"
                    },
                    "nikname": {
                        "type": "string",
                        "format": "email"
                    },
                    "image_url": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string",
                        "maxLength": 255
                    },
                    "experience": {
                        "type": "integer"
                    },
                    "gold": {
                        "type": "integer"
                    },
                    "role_id": {
                        "type": "integer",
                        "format": "int64",
                        "minimum": 1
                    },
                    "rank_id": {
                        "type": "integer",
                        "format": "int64",
                        "minimum": 1
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "nullable": true
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time",
                        "nullable": true
                    },
                    "deleted_at": {
                        "type": "string",
                        "format": "date-time",
                        "nullable": true
                    }
                },
                "type": "object"
            }
        },
        "responses": {
            "ErrorResponse": {
                "description": "Error",
                "content": {
                    "application/json": {
                        "schema": {
                            "required": [
                                "message"
                            ],
                            "properties": {
                                "message": {
                                    "type": "string"
                                }
                            },
                            "type": "object"
                        }
                    }
                }
            },
            "ValidationErrorsResponse": {
                "description": "Validation errors",
                "content": {
                    "application/json": {
                        "schema": {
                            "$ref": "#/components/schemas/ValidationErrors"
                        }
                    }
                }
            },
            "LengthAwarePaginatorResponse": {
                "description": "Length aware paginator",
                "content": {
                    "application/json": {
                        "schema": {
                            "$ref": "#/components/schemas/LengthAwarePaginator"
                        }
                    }
                }
            }
        },
        "parameters": {
            "id": {
                "name": "id",
                "in": "path",
                "required": true,
                "schema": {
                    "type": "integer",
                    "format": "int64",
                    "minimum": 1
                }
            },
            "page": {
                "name": "page",
                "in": "query",
                "required": false,
                "schema": {
                    "type": "integer",
                    "minimum": 1,
                    "nullable": true
                }
            },
            "per_page": {
                "name": "per_page",
                "in": "query",
                "required": false,
                "schema": {
                    "type": "integer",
                    "minimum": -1,
                    "nullable": true
                }
            },
            "in_storage": {
                "name": "in_storage",
                "in": "query",
                "required": false,
                "schema": {
                    "type": "integer",
                    "enum": [
                        1,
                        0
                    ]
                }
            },
            "only_available": {
                "name": "only_available",
                "in": "query",
                "required": false,
                "schema": {
                    "type": "integer",
                    "enum": [
                        1,
                        0
                    ]
                }
            }
        },
        "securitySchemes": {
            "JWT": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT",
                "in": "header"
            }
        }
    },
    "tags": [
        {
            "name": "auth",
            "description": "auth"
        },
        {
            "name": "store",
            "description": "store"
        }
    ]
}